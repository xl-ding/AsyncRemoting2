<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
	</startup>
	<system.runtime.remoting>
		<application>
			<service>
				<wellknown
				   type="ServiceClass, ServiceClass"
				   mode="Singleton"
				   objectUri="ServiceClass"
            />
			</service>
			<channels>
				<channel ref="http"
						 port="8080" />
			</channels>
		</application>
	</system.runtime.remoting>
</configuration>